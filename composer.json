{
    "name": "alz/campaign-monitor-mailer",
    "description": "A Laravel 10 package for sending emails via Campaign Monitor smart emails",
    "type": "library",
    "version": "0.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Dempe",
            "email": "dempeb18@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "campaignmonitor/createsend-php": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "CampaignMonitorMailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CampaignMonitorMailer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CampaignMonitorMailer\\Providers\\CampaignMonitorMailerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}